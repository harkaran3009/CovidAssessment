<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covidassessment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CovidAssessment">
        <activity android:name=".CovidAssessment"></activity>
        <activity android:name=".AssessmentResult"></activity>
        <activity android:name=".VaccineRegisteration"></activity>
        <activity android:name=".VaccineRegisterResult"></activity>
        <activity android:name=".VaccineBookCheck"></activity>
        <activity android:name=".CovidTestBook"></activity>
        <activity android:name=".CovidInfo"></activity>
        <activity android:name=".TestBookStatus"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.CovidAssessment.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>