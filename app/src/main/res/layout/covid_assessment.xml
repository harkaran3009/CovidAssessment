<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="187dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/social_image_en" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/covid_assessment_head"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="519dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/covid_assessment_ques1"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="14sp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="131dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="171dp"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/textView5"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="@string/covid_assessment_ques1_op1"
                                    android:textAlignment="center"
                                    android:textColor="@color/black"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/textView6"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="@string/covid_assessment_ques1_op2"
                                    android:textAlignment="center"
                                    android:textColor="@color/black" />

                                <TextView
                                    android:id="@+id/textView9"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="@string/covid_assessment_ques1_op3"
                                    android:textAlignment="center"
                                    android:textColor="@color/black" />

                                <TextView
                                    android:id="@+id/textView8"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="@string/covid_assessment_ques1_op4"
                                    android:textAlignment="center"
                                    android:textColor="@color/black" />

                                <TextView
                                    android:id="@+id/textView7"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="@string/covid_assessment_ques1_op5"
                                    android:textAlignment="center"
                                    android:textColor="@color/black" />
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="112dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <CheckBox
                                android:id="@+id/checkBox1Yes"
                                android:layout_width="match_parent"
                                android:layout_height="20dp"
                                android:text="@string/covid_assessment_ques1_chkbx1" />

                            <CheckBox
                                android:id="@+id/checkBox2Yes"
                                android:layout_width="match_parent"
                                android:layout_height="35dp"
                                android:text="@string/covid_assessment_ques1_chkbx1" />

                            <CheckBox
                                android:id="@+id/checkBox3Yes"
                                android:layout_width="match_parent"
                                android:layout_height="20dp"
                                android:text="@string/covid_assessment_ques1_chkbx1" />

                            <CheckBox
                                android:id="@+id/checkBox4Yes"
                                android:layout_width="match_parent"
                                android:layout_height="35dp"
                                android:text="@string/covid_assessment_ques1_chkbx1" />

                            <CheckBox
                                android:id="@+id/checkBox5Yes"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/covid_assessment_ques1_chkbx1" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <CheckBox
                                android:id="@+id/checkBox1No"
                                android:layout_width="match_parent"
                                android:layout_height="21dp"
                                android:text="@string/covid_assessment_ques1_chkbx2" />

                            <CheckBox
                                android:id="@+id/checkBox2No"
                                android:layout_width="match_parent"
                                android:layout_height="35dp"
                                android:text="@string/covid_assessment_ques1_chkbx2" />

                            <CheckBox
                                android:id="@+id/checkBox3No"
                                android:layout_width="match_parent"
                                android:layout_height="20dp"
                                android:text="@string/covid_assessment_ques1_chkbx2" />

                            <CheckBox
                                android:id="@+id/checkBox4No"
                                android:layout_width="match_parent"
                                android:layout_height="34dp"
                                android:text="@string/covid_assessment_ques1_chkbx2" />

                            <CheckBox
                                android:id="@+id/checkBox5No"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/covid_assessment_ques1_chkbx2" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="38dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView10"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/covid_assessment_ques2"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textStyle="bold"
                            android:translationY="10dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="31dp"
                        android:orientation="horizontal"
                        android:paddingLeft="70dp"
                        android:paddingRight="25dp">

                        <CheckBox
                            android:id="@+id/checkBox6Yes"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="clip_vertical|start"
                            android:text="@string/covid_assessment_ques1_chkbx1"
                            android:textAlignment="viewStart" />

                        <CheckBox
                            android:id="@+id/checkBox6No"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="@string/covid_assessment_ques1_chkbx2" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView11"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/covid_assessment_ques3"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textStyle="bold"
                            android:translationY="10dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:orientation="horizontal"
                        android:paddingLeft="70dp"
                        android:paddingRight="25dp">

                        <CheckBox
                            android:id="@+id/checkBox7Yes"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingLeft="10dp"
                            android:text="@string/covid_assessment_ques1_chkbx1" />

                        <CheckBox
                            android:id="@+id/checkBox7No"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingLeft="10dp"
                            android:text="@string/covid_assessment_ques1_chkbx2" />
                    </LinearLayout>

                    <Button
                        android:id="@+id/button5"
                        android:layout_width="250dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="@string/covid_assessment_button"
                        android:textAlignment="center"
                        android:translationY="20dp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>